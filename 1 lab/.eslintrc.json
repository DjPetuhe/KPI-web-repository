{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["sonarjs"],
  "extends": ["eslint:recommended", "plugin:sonarjs/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "cognitive-complexity": ["error", 15]
  }
}
